/**app.wxss**/
page {
    --main-bg-color: #DAECEB;
    --main-component-color: #85E4DC;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 100rpx 0;
  box-sizing: border-box;
} 

.lxy-nav-bar__logo {
    align-items: center;
    display: flex;
    height: 32px;
    padding-left: 28rpx;
    position: relative;
}

.lxy-nav-bar__logo image {
    border-radius: 26rpx;
    height: 26px;
    width: 26px;
}