<!--pages/Tabbar/index.wxml-->
<!--miniprogram/custom-tab-bar/index.wxml-->

<text>pages/Tabbar/index.wxml</text>
<button bindtap="wxNavAction">login test</button>

<scroll-view type="custom" style="width: 100%; height: 100%;" bindscrolltolower="bindscrolltolower">
  <grid-view type="masonry" cross-axis-count="{{crossAxisCount}}" cross-axis-gap="{{crossAxisGap}}" main-axis-gap="{{mainAxisGap}}">
    <view wx:for="{{list}}" class="grid-box">
      <image src="{{item.image_url}}" mode="widthFix" binderror="binderror"></image>
      <view class="content-box">
        <text>您去过这儿吗？您配吗</text>
        <view class="profile-text">
          <view class="left">
            <image class="icon" src="http://mmbiz.qpic.cn/mmbiz_jpg/EhZ0Apwc3PE1BWpdFyDHX3IF030WehVgmoX0XicjSL0AYxFUM1sQTZIQTkhzTfCTvicBFUNrQlfllSBLhrOVZ77Q/0?wx_fmt=jpeg"></image>
            <text>snake</text>
          </view>
          <view class="right">
            <image class="icon-collection" src="https://res.wx.qq.com/community/dist/community/images/disagree_icon_fab2b7.svg"></image>
            <text>666</text>
          </view>
        </view>
      </view>
    </view>
  </grid-view>
</scroll-view>