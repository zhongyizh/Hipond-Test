.container-loading {
    height: 100%;
    left: 0;
    position: fixed;
    top: 0;
    width: 100%;
}

.content {
    align-items: center;
    background: #fff;
    display: flex;
    flex-direction: column;
    height: 100%;
    justify-content: center;
}

.spinner-flash-medium {
    height: 60rpx;
    width: 60rpx;
}

.spinner-flash-mini {
    height: 40rpx;
    width: 40rpx;
}

.spinner-flash-large {
    height: 80rpx;
    width: 80rpx;
}

.spinner-flip-medium {
    height: 60rpx;
    width: 60rpx;
}

.spinner-flip-mini {
    height: 40rpx;
    width: 40rpx;
}

.spinner-flip-large {
    height: 80rpx;
    width: 80rpx;
}

.spinner-change-medium {
    height: 30rpx;
    width: 30rpx;
}

.spinner-change-mini {
    height: 20rpx;
    width: 20rpx;
}

.spinner-change-large {
    height: 50rpx;
    width: 50rpx;
}

.flash-spinner {
    position: relative;
}

.flash-bounce1,.flash-bounce2 {
    animation: bounce 2s ease-in-out infinite;
    background-color: #3963bc;
    border-radius: 50%;
    height: 100%;
    left: 0;
    opacity: .6;
    position: absolute;
    top: 0;
    width: 100%;
}

.flash-bounce2 {
    animation-delay: -1s;
}

@-webkit-keyframes bounce {
    0%,100% {
        -webkit-transform: scale(0);
    }

    50% {
        -webkit-transform: scale(1);
    }
}

@keyframes bounce {
    0%,100% {
        transform: scale(0);
    }

    50% {
        transform: scale(1);
    }
}

.flip-bounce1 {
    animation: rotateplane 1.2s ease-in-out infinite;
    background-color: #3963bc;
}

@-webkit-keyframes rotateplane {
    0% {
        -webkit-transform: perspective(120px);
    }

    50% {
        -webkit-transform: perspective(120px) rotateY(180deg);
    }

    100% {
        -webkit-transform: perspective(120px) rotateY(180deg) rotateX(180deg);
    }
}

@keyframes rotateplane {
    0% {
        transform: perspective(120px) rotateX(0) rotateY(0);
    }

    50% {
        transform: perspective(120px) rotateX(-180.1deg) rotateY(0);
    }

    100% {
        transform: perspective(120px) rotateX(-180deg) rotateY(-179.9deg);
    }
}

.change-spinner {
    text-align: center;
    width: 240rpx;
}

.change-bounce1 {
    animation: bouncedelay 1.4s ease-in-out infinite;
    animation-delay: -.32s;
    animation-fill-mode: both;
}

.change-bounce1,.change-bounce2 {
    background-color: #3963bc;
    border-radius: 100%;
    display: inline-block;
}

.change-bounce2 {
    animation: bouncedelay 1.4s ease-in-out infinite;
    animation-delay: -.16s;
    animation-fill-mode: both;
}

.change-bounce3 {
    animation: bouncedelay 1.4s ease-in-out infinite;
    animation-fill-mode: both;
    background-color: #3963bc;
    border-radius: 100%;
    display: inline-block;
}

@-webkit-keyframes bouncedelay {
    0%,80%,100% {
        -webkit-transform: scale(0);
    }

    40% {
        -webkit-transform: scale(1);
    }
}

@keyframes bouncedelay {
    0%,80%,100% {
        transform: scale(0);
    }

    40% {
        transform: scale(1);
    }
}

.spinner-circle {
    position: relative;
}

.spinner-circle-mini {
    height: 40rpx;
    width: 40rpx;
}

.spinner-circle-medium {
    height: 60rpx;
    width: 60rpx;
}

.spinner-circle-large {
    height: 70rpx;
    width: 70rpx;
}

.container1>.container-view,.container2>.container-view,.container3>.container-view {
    animation: bouncedelay4 1.2s ease-in-out infinite;
    animation-fill-mode: both;
    background-color: #3963bc;
    border-radius: 50%;
    position: absolute;
}

.circle-mini {
    height: 12rpx;
    width: 12rpx;
}

.circle-medium {
    height: 15rpx;
    width: 15rpx;
}

.circle-large {
    height: 20rpx;
    width: 20rpx;
}

.spinner-circle .spinner-container {
    height: 100%;
    position: absolute;
    width: 100%;
}

.container2 {
    transform: rotateZ(45deg);
}

.container3 {
    transform: rotateZ(90deg);
}

.circle1 {
    left: 0;
    top: 0;
}

.circle2 {
    right: 0;
    top: 0;
}

.circle3 {
    bottom: 0;
    right: 0;
}

.circle4 {
    bottom: 0;
    left: 0;
}

.container2 .circle1 {
    animation-delay: -1.1s;
}

.container3 .circle1 {
    animation-delay: -1s;
}

.container1 .circle2 {
    animation-delay: -.9s;
}

.container2 .circle2 {
    animation-delay: -.8s;
}

.container3 .circle2 {
    animation-delay: -.7s;
}

.container1 .circle3 {
    animation-delay: -.6s;
}

.container2 .circle3 {
    animation-delay: -.5s;
}

.container3 .circle3 {
    animation-delay: -.4s;
}

.container1 .circle4 {
    animation-delay: -.3s;
}

.container2 .circle4 {
    animation-delay: -.2s;
}

.container3 .circle4 {
    animation-delay: -.1s;
}

@-webkit-keyframes bouncedelay4 {
    0%,80%,100% {
        -webkit-transform: scale(0);
    }

    40% {
        -webkit-transform: scale(1);
    }
}

@keyframes bouncedelay4 {
    0%,80%,100% {
        transform: scale(0);
    }

    40% {
        transform: scale(1);
    }
}

.rotate {
    animation: rotate .7s linear infinite;
    border-radius: 50%;
}

.rotate-mini {
    height: 40rpx;
    width: 40rpx;
}

.rotate-medium,.rotate-mini {
    border-bottom: 6rpx solid #3963bc;
    border-left: 6rpx solid #3963bc;
    border-right: 6rpx solid transparent!important;
    border-top: 6rpx solid #3963bc;
}

.rotate-medium {
    height: 50rpx;
    width: 50rpx;
}

.rotate-large {
    border-bottom: 8rpx solid #3963bc;
    border-left: 8rpx solid #3963bc;
    border-right: 8rpx solid transparent!important;
    border-top: 8rpx solid #3963bc;
    height: 70rpx;
    width: 70rpx;
}

@-webkit-keyframes rotate {
    0% {
        transform: rotate(0);
    }

    100% {
        transform: rotate(1turn);
    }
}

@keyframes rotate {
    0% {
        transform: rotate(0);
    }

    100% {
        transform: rotate(1turn);
    }
}